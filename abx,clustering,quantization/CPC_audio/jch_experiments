python -u cpc/train.py --pathDB /pio/data/zerospeech2021/LibriSpeech/train-clean-100 --pathCheckpoint /pio/scratch/2/jch/wav2vec/runs/cpc/cpc_ctc_try_sl/ --pathTrain /pio/scratch/2/jch/wav2vec/LibriSpeech100_labels_split/train_split.txt --pathVal /pio/scratch/2/jch/wav2vec/LibriSpeech100_labels_split/test_split.txt --file_extension .flac --n_process_loader 8 --max_size_loaded 400000000 --batchSizeGPU 64 --nPredicts 8 --CPCCTC --CPCCTCNumMatched 12 --CPCCTCSelfLoop --CPCCTCSkipBeg 1 --CPCCTCSkipEnd 2 2>&1 | tee -ai /pio/scratch/2/jch/wav2vec/runs/cpc/cpc_ctc_try_sl.log